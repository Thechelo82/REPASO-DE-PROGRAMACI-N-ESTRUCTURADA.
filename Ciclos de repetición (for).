# a
for i in range(1, 11):
    print(i)

# b
suma = 0
for i in range(1, 101):
    suma += i
print(suma)

# c
for i in range(1, 11):
    for j in range(1, 11):
        print(f"{i} x {j} = {i*j}")
    print()

# d
palabra = input()
for letra in palabra:
    print(letra)

# e
n = int(input())
suma = 0
for i in range(1, n+1):
    if i % 2 != 0:
        suma += i
print(suma)
